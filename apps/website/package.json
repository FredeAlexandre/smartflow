{
  "name": "website",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.64.3",
    "@tanstack/start": "^1.64.3",
    "@vitejs/plugin-react": "^4.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vinxi": "^0.4.3"
  },
  "devDependencies": {
    "@smartflow/tsconfig": "workspace:^",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "{projectRoot}/app/**/*.{tsx|ts}",
          "{projectRoot}/app.config.ts",
          "{projectRoot}/tsconfig.json"
        ],
        "outputs": ["{projectRoot}/.output", "{projectRoot}/.vinxi"]
      }
    }
  }
}
